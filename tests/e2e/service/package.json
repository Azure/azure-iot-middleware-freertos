  "license": "ISC",
  "dependencies": {
    "@azure/event-hubs": "^1.0.8",
    "@azure/ms-rest-js": "^2.0.5",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.9.0",
    "azure-iot-common": "^1.12.9",
    "azure-iothub": "^1.14.2",
    "chai": "^4.2.0",
    "mocha": "^7.1.1",
    "terminate": "^2.1.2",
    "typescript": "^3.8.3",
    "yargs": "^15.3.0",
    "minimist": "^1.2.5",
    "azure-iot-provisioning-service": "1.8.9"
  },
  "devDependencies": {},
  "description": ""
}
